extends layout

block content
    div#content.max1140
        div.select-wrap
            h5.instruction-txt Select a Conversation
            div.dd-wrap
                div.selectbox
                    span.default-txt Select file
                    span.selected-txt.hide sample file 1
                    i.fa.fa-chevron-down

                div.dd
                    ul
                        li#file1.clearfix
                            span.file-name sample file 1
                            span.process(title="start processing")
                                i.fa.fa-play
                        li#file2.clearfix
                            span.file-name sample file 2
                            span.process(title="start processing")
                                i.fa.fa-play
                        li#file3.clearfix
                            span.file-name sample file 3
                            span.process(title="start processing")
                                i.fa.fa-play
                        li#file4.clearfix
                            span.file-name sample file 4
                            span.process(title="start processing")
                                i.fa.fa-play

        div
            div#waveform 

        div.afilelist-wrap
            ul.afile-list
                li
                    h4.sample-name sample 1 goes here
                    ul.tag-list
                        li.col-xs-2.tag.unit-size
                            i
                            span.txt Property Requirements - Unit Size
                        li.col-xs-2.tag.budget
                            i
                            span.txt Property Requirements - Budget
                        li.col-xs-2.tag.location
                            i
                            span.txt Property Requirements - Location
                        li.col-xs-2.tag.furnishing
                            i
                            span.txt Property Requirements - Furnishing
                        li.col-xs-2.tag.possession.active
                            i
                            span.txt Property Requirements - Possession
                        li.col-xs-2.tag.sale-type
                            i
                            span.txt Property Requirements - Sale Type
                        li.col-xs-2.tag.parking
                            i
                            span.txt Property Requirements - Parking
                        li.col-xs-2.tag.home-loan
                            i
                            span.txt Home Loan
                        li.col-xs-2.tag.site-visit
                            i
                            span.txt Site Visit
                        li.col-xs-2.tag.number-change
                            i
                            span.txt Phone Number Exchange
                        li.col-xs-2.tag.call-back
                            i
                            span.txt Ask for a Call back
                        li.col-xs-2.tag.commission
                            i
                            span.txt Broker Commission
                        li.col-xs-2.tag.not-available
                            i
                            span.txt Property Not Available
                        li.col-xs-2.tag.reference
                            i
                            span.txt Reference to a Listing
                        li.col-xs-2.tag.token
                            i
                            span.txt Transaction - Token Money
            div.clearfix


    script(type="text/javascript", src="/js/second.js")




